{
  "name": "snowbox",
  "version": "2.0.1",
  "description": "universal UI system",
  "main": "dist",
  "scripts": {
    "dev": "dumi dev",
    "docs:build": "dumi build"
  },
  "keywords": [
    "react-native",
    "react-native-web"
  ],
  "files": [
    "dist",
    "static"
  ],
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "yuxz",
  "license": "MIT",
  "dependencies": {
    "compare-versions": "^4.1.3",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lottie-react-native": "^5.1.3",
    "lottie-web": "^5.9.6",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
    "react": "16.9",
    "react-dom": "16.9",
    "react-native": "0.61.4",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-scrollable-tab-view": "^1.0.0",
    "react-native-svg": "^12.4.0",
    "react-native-web": "0.16",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.5",
    "@types/react": "16",
    "@types/react-native": "^0.70.4",
    "@umijs/test": "^3.0.5",
    "babel-plugin-react-native-web": "0.16",
    "dumi": "^1.1.0",
    "dumi-theme-mobile": "^1.1.24",
    "father-build": "^1.19.1",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
